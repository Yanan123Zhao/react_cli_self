{
  "presets": [
    ["@babel/preset-env", {
      "modules":false
    }],
    "@babel/preset-react"
  ],
  "plugins": [
      ["@babel/plugin-transform-runtime",{
        "corejs": 3
        }
      ], 
      ["@babel/plugin-proposal-class-properties",
        {"loose": true}
      ],
      ["import", {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true // `style: true` 会加载 less 文件
      },
      "react-hot-loader/babel"
    ]
  ]
}
